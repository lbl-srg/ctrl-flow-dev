{
  "dependencies": {
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "dotenv": "^14.2.0",
    "express": "^4.17.2",
    "helmet": "^5.0.1",
    "jsonpath": "^1.1.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "tmp": "^0.2.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.8",
    "@types/supertest": "^2.0.11",
    "@types/tmp": "^0.2.3",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "scripts": {
    "build": "tsc",
    "preparseTemplates": "sh ./bin/convert-modelica-to-json.sh",
    "parseTemplates": "node scripts/parse-templates.js",
    "start": "nodemon src/index.ts",
    "install-modelica-deps": "./bin/install-modelica-dependencies.sh",
    "docker:start": "docker-compose -f docker/docker-compose.yaml --env-file ./.env up -d",
    "docker:debug": "docker-compose -f docker/docker-compose.yaml -f docker/docker-compose.debug.yaml --env-file ./.env up -d",
    "docker:stop": "docker-compose -f docker/docker-compose.yaml down",
    "docker:logs": "docker-compose -f docker/docker-compose.yaml logs -f",
    "docker:rebuild": "docker-compose -f docker/docker-compose.yaml build --pull --no-cache",
    "docker:shell": "docker exec -it linkage-widget /bin/sh",
    "debug": "nodemon --exec 'node -r ts-node/register --inspect=0.0.0.0:9229 src/index.ts'",
    "format": "prettier --config ../.prettierrc.json --ignore-path ../.prettierignore --write .",
    "test": "npm run integration-test",
    "integration-test": "jest"
  }
}
