                  "type_specifier": "Buildings.Controls.OBC.ASHRAE.G36.Types.MultizoneAHUMinOADesigns",
                  "component_list": [
                    {
                      "declaration": {
                        "identifier": "minOADes",
                        "modification": {
                          "equal": true,
                          "expression": {
                            "if_expression": {
                              "if_elseif": [
                                {
                                  "condition": {
                                    "simple_expression": "secOutRel.typSecOut == Buildings.Templates.AirHandlersFans.Types.OutdoorSection.SingleDamper"
                                  },
                                  "then": {
                                    "simple_expression": "Buildings.Controls.OBC.ASHRAE.G36.Types.MultizoneAHUMinOADesigns.CommonDamper"
                                  }
                                },
                                {
                                  "condition": {
                                    "simple_expression": "secOutRel.typSecOut == Buildings.Templates.AirHandlersFans.Types.OutdoorSection.DedicatedDampersAirflow"
                                  },
                                  "then": {
                                    "simple_expression": "Buildings.Controls.OBC.ASHRAE.G36.Types.MultizoneAHUMinOADesigns.SeparateDamper_AFMS"
                                  }
                                },
                                {
                                  "condition": {
                                    "simple_expression": "secOutRel.typSecOut == Buildings.Templates.AirHandlersFans.Types.OutdoorSection.DedicatedDampersPressure"
                                  },
                                  "then": {
                                    "simple_expression": "Buildings.Controls.OBC.ASHRAE.G36.Types.MultizoneAHUMinOADesigns.SeparateDamper_DP"
                                  }
                                }
                              ],
                              "else_expression": {
                                "simple_expression": "Buildings.Controls.OBC.ASHRAE.G36.Types.MultizoneAHUMinOADesigns.CommonDamper"
                              }
                            }
                          }
                        }
