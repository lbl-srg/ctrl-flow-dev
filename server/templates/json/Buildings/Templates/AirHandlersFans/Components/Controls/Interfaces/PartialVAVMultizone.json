              },
              {
                "final": true,
                "component_clause": {
                  "type_prefix": "parameter",
                  "type_specifier": "Buildings.Controls.OBC.ASHRAE.G36.Types.BuildingPressureControlTypes",
                  "component_list": [
                    {
                      "declaration": {
                        "identifier": "buiPreCon",
                        "modification": {
                          "equal": true,
                          "expression": {
                            "if_expression": {
                              "if_elseif": [
                                {
                                  "condition": {
                                    "simple_expression": "secOutRel.typSecRel == Buildings.Templates.AirHandlersFans.Types.ReliefReturnSection.ReliefDamper"
                                  },
                                  "then": {
                                    "simple_expression": "Buildings.Controls.OBC.ASHRAE.G36.Types.BuildingPressureControlTypes.ReliefDamper"
                                  }
                                },
                                {
                                  "condition": {
                                    "simple_expression": "secOutRel.typSecRel == Buildings.Templates.AirHandlersFans.Types.ReliefReturnSection.ReliefFan"
                                  },
                                  "then": {
                                    "simple_expression": "Buildings.Controls.OBC.ASHRAE.G36.Types.BuildingPressureControlTypes.ReliefFan"
                                  }
                                },
                                {
                                  "condition": {
                                    "simple_expression": "secOutRel.typSecRel == Buildings.Templates.AirHandlersFans.Types.ReliefReturnSection.ReturnFan"
                                  },
                                  "then": {
                                    "simple_expression": "(if typCtlFanRet == Buildings.Templates.AirHandlersFans.Types.ControlFanReturn.AirflowTracking then Buildings.Controls.OBC.ASHRAE.G36.Types.BuildingPressureControlTypes.ReturnFanAir elseif typCtlFanRet == Buildings.Templates.AirHandlersFans.Types.ControlFanReturn.BuildingPressure then Buildings.Controls.OBC.ASHRAE.G36.Types.BuildingPressureControlTypes.ReturnFanDp else Buildings.Controls.OBC.ASHRAE.G36.Types.BuildingPressureControlTypes.ReliefDamper)"
                                  }
                                }
                              ],
                              "else_expression": {
                                "simple_expression": "Buildings.Controls.OBC.ASHRAE.G36.Types.BuildingPressureControlTypes.ReliefDamper"
                              }
                            }
                          }
                        }
